    "react": "^15.1.0",
    "react-dom": "^15.4.2",
    "react-native": "^0.40.0",
    "react-native-router-flux": "^3.37.0",
    "react-native-vector-icons": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^3.1.0",
    "eslint-loader": "^1.3.0",
