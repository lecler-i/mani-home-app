    "react-native-router-flux": "^3.38.0",
    "react-native-vector-icons": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
